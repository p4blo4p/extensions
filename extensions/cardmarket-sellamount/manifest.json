{
  "manifest_version": 3,
  "name": "Mi Extension",
  "description": "Descripción de la extensión",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://tusitio.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
